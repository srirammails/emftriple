<plugin>
    <extension
        point="org.eclipse.emf.ecore.generated_package">
      <package
            uri="http://www.emf4sw.org/2009/OWL"
            class="com.emf4sw.owl.OWLPackage"
            genModel="model/owl.genmodel">
      </package>
    </extension>
    
    <extension
		point="org.eclipse.emf.ecore.extension_parser">
		<parser
			class="com.emf4sw.owl.resource.OWLXMLResourceFactory"
			type="owl">
		</parser>
	</extension>
	
	<extension
		point="org.eclipse.emf.ecore.extension_parser">
		<parser
			class="com.emf4sw.owl.resource.OWLTurtleResourceFactory"
			type="ttl">
		</parser>
	</extension>
	
	
</plugin>
